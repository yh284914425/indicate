{
  "name": "indicate",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:unit": "vitest",
    "coverage": "vitest run --coverage",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "build:node": "esbuild server/index.ts --bundle --platform=node --target=node16 --outfile=dist/server.cjs --external:ws --external:util --external:stream --external:events --external:http --external:https --external:net --external:tls --external:crypto",
    "start:node": "node dist/server.cjs",
    "start:pm2": "pm2 start ecosystem.config.cjs",
    "stop:pm2": "pm2 stop crypto-divergence",
    "restart:pm2": "pm2 restart crypto-divergence",
    "logs:pm2": "pm2 logs crypto-divergence",
    "deploy": "vercel --prod"
  },
  "dependencies": {
    "@css-render/vue3-ssr": "^0.15.14",
    "@types/ws": "^8.5.13",
    "@vicons/ionicons5": "^0.13.0",
    "axios": "^1.7.9",
    "echarts": "^5.4.3",
    "esbuild": "^0.24.2",
    "lightweight-charts": "^4.2.2",
    "naive-ui": "^2.40.3",
    "pinia": "^2.2.6",
    "technicalindicators": "^3.1.0",
    "vue": "^3.5.13",
    "vue-echarts": "^6.6.8",
    "vue-router": "^4.4.5",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.9.3",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "jsdom": "^25.0.1",
    "npm-run-all2": "^7.0.1",
    "sass": "^1.83.0",
    "typescript": "~5.6.3",
    "vite": "^6.0.1",
    "vite-plugin-vue-devtools": "^7.6.5",
    "vitest": "^2.1.5",
    "vue-tsc": "^2.1.10"
  }
}
